{
    "name": "hdw/import-dms-plugin-boilerplate",
    "description": "Import [Project] Products from HDW DMS",
    "type": "wp-plugin",
    "authors": [
        {
            "name": "Anthony Spross",
            "email": "a.spross@hdw1.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "HDW\\ProjectDmsImporter\\": "src/"
        },
        "files": [
            "inc/functions.php"
        ]
    },
    "require": {
        "ralfhortt/wp-plugin": "^1.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "extra": {
        "installer-paths": {
            "web/app/mu-plugins/{$name}/": [
                "type:wordpress-muplugin"
            ],
            "web/app/plugins/{$name}/": [
                "type:wordpress-plugin"
            ]
        }
    }
}